{
  "manifest_version": 2,

  "name": "History Cleaner",
  "description": "This extension clears browser history with search term.",
  "version": "1.0",
  
  "permissions": [
    "history",
    "background" ,"https://*/*","tabs"
  ],
  "background": {
        "scripts": ["background.js"]
    },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}